# 前端环境变量配置

# 后端API地址
VITE_API_URL=http://localhost:3000/api

# 游戏配置
VITE_GAME_VERSION=1.0.0
VITE_DEBUG_MODE=true

# 存储配置
VITE_STORAGE_MODE=indexeddb
# 可选值: local (LocalStorage) | indexeddb (IndexedDB) | cloud (云端)

VITE_ENABLE_COMPRESSION=true
# 是否启用数据压缩 (>100KB)

VITE_ENABLE_CLOUD_SYNC=false
# 是否启用云同步功能

VITE_USERNAME=Player
# 默认用户名
